extends layouts/main
include mixins/videoBlock

block content
    div.search__header
        h3 Searching for #{searchingBy}
    div.search__videos
        if videos.length === 0
            h5 No Videos Found
        each video in videos
            +videoBlock({
                title:video.title,
                views:video.views,
                id:video.id,
                videoFile:video.fileUrl
            })